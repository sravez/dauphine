   "source": [
    "# Exercice 8.6\n",
    "\n",
    "L'exécution de `g` incrémente `x` et affiche `[1,4,4]`.\n",
    "\n",
    "L'exécution de `f` affiche `[1,6,4]`, le deuxième élément étant `x+1`"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 1,
   "id": "aa60fb98",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "[1, 4, 4]\n",
      "[1, 6, 4]\n"
     ]
    }
   ],
   "source": [
    "def f():\n",
    "    L = [1,x,4]\n",
    "    L[1] += 1\n",
    "    print(L)\n",
    "def g():\n",
    "    global x\n",
    "    L = [1,x,4]\n",
    "    x += 1\n",
    "    print(L)\n",
    "\n",
    "x = 4\n",
    "g()\n",
    "f()"
   ]
  }
 ],
